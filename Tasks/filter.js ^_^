// Filter Array by type name
//Step 1: Rename function and variables

filterByType = (unfilteredArray, filterType) => {
  remove = [];
  for (It of unfilteredArray) {
    x = unfilteredArray.indexOf(It);
    if (typeof unfilteredArray[x] !== filterType) {
      remove.unshift(x);
    }
  }
  for (x of remove) unfilteredArray.splice(x, 1);
  return unfilteredArray;
};


module.exports = filterByType;
