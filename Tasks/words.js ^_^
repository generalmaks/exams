// Count words in a string
//Step 1: Remove useless code

Words = function (s) {
  number_of_words_in_s = 0;
  flag = false;
  for (c of s) {
    if (!flag) {
      if (c !== ' ') {
        flag = true;
        number_of_words_in_s++;
      }
    } else {
      if (c === ' ') {
        flag = false;
      }
    }
  }
  return number_of_words_in_s;
};


module.exports = Words;
